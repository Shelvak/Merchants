<h1>Caja Diaria</h1>

<table>
  <tr>
    <th>Día</th>
    <th>Mes</th>
    <th>Año</th>
    <th>Cantidad</th>
    <th>Total</th>
  </tr>

<% @boxes.each do |box| %>
  <tr>
    <td><%= box.day %></td>
    <td><%= box.month %></td>
    <td><%= box.year %></td>
    <td><%= box.count %></td>
    <td><%= number_to_currency(box.total) %></td>
  </tr>
<% end %>
</table>

<br />

